{
  "env": {
      "browser": true,
      "commonjs": true,
      "node": true
  },
  "extends": [
    "react-app",
    "eslint:recommended"
  ],
  "plugins": [
      "react"
  ],
  "rules": {
      "indent": [
          "error",
          2
      ],
      "linebreak-style": [
          "error",
          "unix"
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "never"
      ],
      "react/jsx-uses-vars": ["error"]
  }
}